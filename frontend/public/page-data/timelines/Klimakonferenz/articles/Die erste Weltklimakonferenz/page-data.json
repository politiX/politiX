    "result": {"data":{"allStrapiArticle":{"edges":[{"node":{"id":"Article_61a6018da1fa51001667ef95","micro_article_container":[{"text":"Die Rede von einem menschengemachten Klimawandels war 1979 noch nicht weit verbreitet.","image":{"name":"klima.png","alternativeText":"","caption":"","hash":"klima_b17d88f453","ext":".png","mime":"image/png","size":21.58,"width":512,"height":512,"url":"https://res.cloudinary.com/politi-x/image/upload/v1641373916/klima_b17d88f453.png","provider_metadata":{"public_id":"klima_b17d88f453","resource_type":"image"},"formats":{"thumbnail":{"name":"thumbnail_klima.png","hash":"thumbnail_klima_b17d88f453","ext":".png","mime":"image/png","width":156,"height":156,"size":7.9,"path":null,"url":"https://res.cloudinary.com/politi-x/image/upload/v1641373917/thumbnail_klima_b17d88f453.png","provider_metadata":{"public_id":"thumbnail_klima_b17d88f453","resource_type":"image"}},"small":{"name":"small_klima.png","hash":"small_klima_b17d88f453","ext":".png","mime":"image/png","width":500,"height":500,"size":36.64,"path":null,"url":"https://res.cloudinary.com/politi-x/image/upload/v1641373918/small_klima_b17d88f453.png","provider_metadata":{"public_id":"small_klima_b17d88f453","resource_type":"image"}}},"provider":"cloudinary","related":[],"createdAt":"2022-01-05T09:11:58.702Z","updatedAt":"2022-01-05T13:02:19.103Z","id":"61d560deb8f2fa0016502bb1","localFile___NODE":"b7b41e00-2700-5402-ac0f-6599cd8707e4"},"id":"61a6018ea1fa51001667ef96","strapi_component":"timeline.micro-article"},{"text":"Trotzdem trafen sich am 12. Februar Delegierte in Genf zur ersten Weltklimakonferenz. ","image":{"name":"datum.png","alternativeText":"","caption":"","hash":"datum_671ce7354c","ext":".png","mime":"image/png","size":7.58,"width":512,"height":512,"url":"https://res.cloudinary.com/politi-x/image/upload/v1641373933/datum_671ce7354c.png","provider_metadata":{"public_id":"datum_671ce7354c","resource_type":"image"},"formats":{"thumbnail":{"name":"thumbnail_datum.png","hash":